{
  // Required
  "manifest_version": 2,
  "name": "SPRITEs-Extension",
  "version": "1.0",

  // Recommended
  //"default_locale": "en",
  "description": "My test extension for SPRITEs",
  "offline_enabled": true,
  //"icons": {...},

  // // Optional
  // "action": ...,
  // "author": ...,
  // "automation": ...,
  "background": {
    "scripts": [
      "background/js/vars.js",
      "background/js/webparser.js",
      "background/js/keymapping.js",
      "background/js/background.js"
    ],
    "persistent": false
  },
  // "background_page": ...,
  // "chrome_settings_overrides": {...},
