  //   }
  // },
  // "chrome_url_overrides": {...},
  // "commands": {...},
  // "content_capabilities": ...,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": ["content_scripts/css/content.css"],
      "js": ["content_scripts/js/content.js"],
      "run_at":     "document_idle",
      "all_frames": false
    }
  ],

  // Pick one (or none)
